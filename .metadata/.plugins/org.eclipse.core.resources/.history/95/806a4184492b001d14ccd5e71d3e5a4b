package jpa.product.dto;


import io.swagger.v3.oas.annotations.media.Schema;
import lombok.AllArgsConstructor;
import lombok.Builder;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@NoArgsConstructor
@AllArgsConstructor
@Builder
@Schema("상품 응답")
public class ProductResponseDTO {

	private Long number;
	private String name;
	private int price;
	private int stock;
	
	
}
